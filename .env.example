ALPHAGATE_API_KEY=
ALPHAGATE_BASE_URL=https://api.alphagate.ai/v1
PORT=8787

# AWS S3 (production uploads)
AWS_REGION=
AWS_S3_BUCKET=
AWS_S3_PREFIX=frames/
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
# TTL for presigned GET/PUT in seconds (e.g., 600)
UPLOAD_SIGN_TTL_SECONDS=600

# Edge API authentication (Bearer tokens). Comma-separated list for dev/testing.
# Example: EDGE_API_KEYS=dev_key_123,another_key
EDGE_API_KEYS=

# Basic per-key rate limit (requests/min). Only enforced if EDGE_API_KEYS is set.
REQUESTS_PER_MINUTE=120

# Web dashboard admin (minimal fallback auth for MVP; use Better Auth later)
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=changeme

# Web dashboard â†’ Edge API calls
EDGE_BASE_URL=http://localhost:8787
# Optional Bearer token for admin APIs; must match one of EDGE_API_KEYS
EDGE_DASHBOARD_API_KEY=

# Supabase Postgres / Kysely
# Full Postgres connection string from Supabase (use SSL / require)
# Example: postgresql://postgres:password@db.<hash>.supabase.co:5432/postgres?sslmode=require
DATABASE_URL=

# If not using sslmode in DATABASE_URL, set to true to enforce SSL in client code
DB_SSL=true

# Supabase project (optional unless you use supabase-js or CLI automations)
SUPABASE_URL=
SUPABASE_ANON_KEY=
# Server-side only. Do NOT expose to clients.
SUPABASE_SERVICE_ROLE_KEY=

# Supabase CLI (optional, for migrations/typegen against your project)
# https://supabase.com/docs/guides/cli
SUPABASE_ACCESS_TOKEN=
SUPABASE_PROJECT_ID=
# Schema to generate types for (default: public)
SUPABASE_DB_SCHEMA=public

# Better Auth (dashboard auth in apps/web)
# Secret used to sign/encrypt sessions/JWTs. Generate: `openssl rand -base64 32`
AUTH_SECRET=
# Public URL of the web app (used for callbacks/cookies). Example: http://localhost:3000
AUTH_URL=
# Cookie name and security flags (tune per env). Secure=true in production over HTTPS.
AUTH_COOKIE_NAME=st_auth
AUTH_SECURE_COOKIES=false
AUTH_COOKIE_DOMAIN=
